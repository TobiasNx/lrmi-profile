{
    "$id": "https://dini-ag-kim.github.io/lrmi-profile/draft/schemas/type.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Type",
    "description": "The type (rdf:type / @type) of the learning resource, taken from sub-classes of sdo:CreativeWork.",
    "type": "array",
    "default": "LearningResource",
    "items": {
        "type": "string",
        "enum": [
            "AudioObject",
            "Book",
